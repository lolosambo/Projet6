<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Entity\Medias">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <field name="trickId" type="integer"/>
        <field name="type" type="string" length="255"/>
        <field name="url" type="string" length="255" />
        <field name="aLaUne" type="string" length="255" />
        <many-to-one field="trick" target-entity="App\Entity\Tricks" inversed-by="medias">
            <join-column name="trick_id" referenced-column-name="id" />
        </many-to-one>


    </entity>
</doctrine-mapping>